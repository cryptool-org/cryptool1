    LTEXT           "Static",IDC_CHECK_PRIME,238,66,95,8
    GROUPBOX        "Parámetros RSA",IDC_STATIC_RSA,9,87,333,101
    LTEXT           "Longitud",IDC_STATIC_RSA0,19,101,75,8,NOT WS_GROUP
