\par {\pntext\pard\plain\v\f5\fs16 \hich\af5\dbch\af0\loch\f5 -\tab}}\pard \fi-360\li360\nowidctlpar\tx0\jclisttab\tx360\tx708\tx1416\tx2124\tx2832\tx3540\tx4248\tx4956\tx5664\tx6372\tx7080\tx7788\tx8496{\*\pn \pnlvlblt\ilvl0\ls21\pnrnot0\pnstrike0
\pnfs16\pnstart1\pnindent360\pnhang{\pntxtb -}}\hyphpar0\ls21\adjustright {\f5 Die erste Zeile enth\'e4lt den Anfang des Chiffrats.
\par {\pntext\pard\plain\v\f5\fs16 \hich\af5\dbch\af0\loch\f5 -\tab}}\pard \fi-360\li360\nowidctlpar\tx0\jclisttab\tx360\tx708\tx1416\tx2124\tx2832\tx3540\tx4248\tx4956\tx5664\tx6372\tx7080\tx7788\tx8496{\*\pn \pnlvlblt\ilvl0\ls21\pnrnot0\pnstrike0
