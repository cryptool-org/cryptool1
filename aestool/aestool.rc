    CONTROL         "Passwort verbergen",IDC_RADIO_PHIDE,"Button",BS_AUTORADIOBUTTON,156,41,79,10
    LTEXT           "Verschlüsseln einer Datei",IDC_STATIC_TITLE,15,9,80,8
    GROUPBOX        "Schlüssel [maximal 64 Hexzeichen (0, 1, ..., 8, 9, A, B, C, D, E, F)]",IDC_STATIC,15,28,290,54
    GROUPBOX        "Eingabedatei",IDC_STATIC,15,94,290,38
